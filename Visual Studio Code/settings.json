{
  "chat.commandCenter.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80
  ],
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "mixed",
  "files.associations": {
    "*.bttpreset": "json",
    "*.jsm": "javascript",
    "*.strings": "plist",
    "*.textClipping": "plist"
  },
  "files.exclude": {
    "**/.git": false
  },
  "git.allowForcePush": true,
  "git.openRepositoryInParentFolders": "never",
  "javascript.preferences.quoteStyle": "single",
  "markdown.updateLinksOnFileMove.enabled": "prompt",
  "markdown.validate.enabled": true,
  "markdown.validate.unusedLinkDefinitions.enabled": "warning",
  "outline.collapseItems": "alwaysCollapse",
  "prettier.singleQuote": true,
  "search.followSymlinks": false,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "window.newWindowProfile": "Default",
  "workbench.colorCustomizations": {
    "terminal.background": "#000000",
    "terminal.foreground": "#00FF00"
  },
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.settings.applyToAllProfiles": [
    "chat.commandCenter.enabled",
    "editor.formatOnPaste",
    "editor.inlayHints.enabled",
    "editor.minimap.enabled",
    "editor.renderWhitespace",
    "editor.rulers",
    "explorer.autoReveal",
    "explorer.confirmDragAndDrop",
    "explorer.fileNesting.enabled",
    "explorer.sortOrder",
    "files.associations",
    "git.allowForcePush",
    "git.openRepositoryInParentFolders",
    "javascript.preferences.quoteStyle",
    "markdown.updateLinksOnFileMove.enabled",
    "markdown.validate.enabled",
    "markdown.validate.unusedLinkDefinitions.enabled",
    "outline.collapseItems",
    "prettier.singleQuote",
    "search.followSymlinks",
    "security.workspace.trust.enabled",
    "security.workspace.trust.untrustedFiles",
    "telemetry.telemetryLevel",
    "terminal.integrated.confirmOnExit",
    "terminal.integrated.cursorBlinking",
    "typescript.updateImportsOnFileMove.enabled",
    "update.mode",
    "update.showReleaseNotes",
    "window.autoDetectColorScheme",
    "window.newWindowProfile",
    "workbench.colorCustomizations",
    "workbench.colorTheme",
    "workbench.editor.enablePreview",
    "workbench.editor.highlightModifiedTabs",
    "workbench.preferredDarkColorTheme",
    "workbench.preferredLightColorTheme"
  ],
  "workbench.startupEditor": "none"
}