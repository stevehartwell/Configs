{
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.preferredLightColorTheme": "Visual Studio Light",
  "workbench.editorAssociations": {
    "*.mp3": "vscode.audioPreview"
  },
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.colorCustomizations": {
    "terminal.background": "#000000",
    "terminal.foreground": "#00FF00"
  },
  "security.workspace.trust.enabled": false,
  "extensions.autoUpdate": false,
  "files.associations": {
    "*.jsm": "javascript",
    "*.bttpreset": "json"
  },
  "breadcrumbs.enabled": true,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.formatOnPaste": true,
  "editor.rulers": [
    80
  ],
  "git.autofetch": true,
  "telemetry.telemetryLevel": "off",
  "window.nativeTabs": true,
  "window.titleBarStyle": "native",
  "window.clickThroughInactive": false,
  "window.autoDetectColorScheme": true,
  "window.zoomLevel": 1,
  "javascript.preferences.quoteStyle": "single",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.surveys.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "css.format.spaceAroundSelectorSeparator": true,
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "svg.preview.mode": "svg",
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.sortOrder": "mixed",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "yaml-plus-json": {
    "keepOriginalFiles": "always",
    "convertOnRename": true,
    "yamlIndent": 2,
    "fileExtensions": {
      "yaml": ".yaml",
      "json": ".json"
    }
  },
  "xmlTools.splitAttributesOnFormat": true,
  "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
  "makefile.configureAfterCommand": false,
  "makefile.configureOnEdit": false,
  "makefile.configureOnOpen": false,
  "python.analysis.autoSearchPaths": false,
  "python.diagnostics.sourceMapsEnabled": true,
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordBasedSuggestions": "off"
  },
  "diffEditor.maxComputationTime": 0,
  "diffEditor.ignoreTrimWhitespace": false,
  "pandoc.render.openViewer": false,
  "window.customTitleBarVisibility": "never",
  "diffEditor.renderSideBySide": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "workbench.editor.enablePreview": false,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "files.exclude": {
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false,
    "**/Icon?": true
  },
  "lldb.suppressUpdateNotifications": true
}
